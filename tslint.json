{
  "extends": ["tslint:latest", "tslint-react"],
  "rules": {
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "arrow-parens": false,
    "no-implicit-dependencies": [true, "dev"],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-bitwise": false,
    "member-access": false,
    "no-empty": false,
    "no-empty-interface": false,
    "curly": false,
    "max-classes-per-file": false,
    "semicolon": false,
    "jsx-no-multiline-js": false,
    "jsx-no-lambda": false
  }
}
